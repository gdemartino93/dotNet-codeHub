@if (TempData["success"] != null)
{
        <div class="alert alert-success notifica-animazione" role="alert">
            @TempData["success"]
        </div>
}
@if (TempData["error"] != null)
{
    <div class="alert alert-danger notifica-animazione" role="alert">
        @TempData["error"]
    </div>

}